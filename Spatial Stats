library(rgdal)
library(sp)
library(spdep)
library(raster)
library(tidyverse)
library(leaflet)

shape_file <- readOGR('C://Users//ryanm//Downloads//ca-county-boundaries//CA_Counties//CA_Counties_TIGER2016.shp')


fire_data <- read_csv("C:/Users/ryanm/Downloads/fire_data.csv")
fire_data <- dplyr::rename(fire_data, NAMELSAD = incident_county)

data <- sp::merge(shape_file,fire_data, by = 'NAMELSAD')

summary(data)
proj4string(data) <-CRS("+init=epsg:27700")
csr <-slot(data,"proj4string")

